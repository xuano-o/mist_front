    "lint:eslint": "eslint . --cache --fix",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss,vue}\" --cache --fix",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "commit": "git cz",
    "release": "bumpp"
  },
